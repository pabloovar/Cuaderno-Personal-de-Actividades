  keywords        = {Animals; Betacoronavirus, genetics; Chlorocebus aethiops; Epigenesis, Genetic; RNA Processing, Post-Transcriptional; RNA, Viral, genetics; SARS-CoV-2; Sequence Analysis, RNA; Transcriptome; Vero Cells; COVID-19; RNA modification; SARS-CoV-2; coronavirus; direct RNA sequencing; discontinuous transcription; epitranscriptome; nanopore; poly(A) tail; transcriptome},
  nlm-id          = {0413066},
  owner           = {NLM},
  pii             = {S0092-8674(20)30406-2},
